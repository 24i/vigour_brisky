{
  "name": "brisky",
  "version": "1.4.0",
  "description": "Brisky is a lightning fast js library for building state driven user interfaces.",
  "main": "index.js",
  "scripts": {
    "test": "(ducktape; node test) | tap-difflet && standard",
    "dev": "nodemon test | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "docs": "node_modules/vigour-doc/bin/vdoc",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "ISC",
  "dependencies": {
    "bubleify": "0.5.1",
    "brisky-class": "^1.3.0",
    "brisky-core": "^1.8.3",
    "brisky-events": "^1.3.0",
    "brisky-props": "^1.1.2",
    "brisky-style": "^1.2.1",
    "brisky-scroll": "https://github.com/vigour-io/brisky-scroll",
    "case-parser": "^1.0.0",
    "ua-cases": "^1.1.2",
    "vigour-ua": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/vigour-io/brisky/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky#readme",
  "keywords": [
    "reactive",
    "state",
    "element",
    "DOM"
  ],
  "browserify": {
    "transform": [
      "bubleify"
    ]
  },
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io>"
  ],
  "devDependencies": {
    "ducktape": "^1.0.0",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "nodemon": "^1.9.1",
    "pre-commit": "^1.1.3",
    "standard": "^7.0.1",
    "tap-difflet": "0.4.0",
    "tape": "^4.4.0",
    "vigour-doc": "^1.1.5",
    "parse-element": "^2.0.1"
  }
}
